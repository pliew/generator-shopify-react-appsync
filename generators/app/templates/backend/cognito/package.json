{
  "name": "campaignpanel-appsync",
  "version": "1.0.0",
  "description": "campaignpanel-appsync (built using Shopify AppSync Yeoman generator)",
  "main": "index.js",
  "repository": "https://github.com/buggy/generator-shopify-react-appsync",
  "author": "Rich Buggy<rich@buggy.id.au>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p ./tsconfig.json",
    "test": "jest --coverage"
  },
  "dependencies": {
    "jsonwebtoken": "8.3.0",
    "source-map-support": "0.5.9"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.13",
    "@types/jest": "23.3.2",
    "@types/jsonwebtoken": "7.2.8",
    "@types/node": "8.10.12",
    "@types/node-fetch": "2.1.2",
    "apollo-codegen": "0.20.2",
    "aws-sdk": "2.290.0",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "1.6.6",
    "jest-raw-loader": "^1.0.1",
    "raw-loader": "^0.5.1",
    "serverless": "1.32.0",
    "serverless-iam-roles-per-function": "1.0.4",
    "serverless-webpack": "5.2.0",
    "ts-jest": "23.1.4",
    "ts-loader": "5.1.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "webpack": "4.19.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "\\.graphql$": "jest-raw-loader",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
